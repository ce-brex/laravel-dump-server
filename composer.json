{
    "name": "ce-brex/laravel-zero-dump-server",
    "description": "Symfony Var-Dump Server for Laravel Zero",
    "keywords": [
        "laravel-zero",
        "laravel-dump-server",
        "laravel-zero-dump-server"
    ],
    "homepage": "https://github.com/ce-brex/laravel-zero-dump-server",
    "license": "MIT",
    "authors": [
        {
            "name": "Bill Rex",
            "email": "brex@clintonelectronics.com",
            "homepage": "https://www.clintonelectronics.com",
            "role": "Developer"
        },
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel-zero/framework": "5.6.*",
        "illuminate/log": "5.6.*",
        "symfony/var-dumper": "^4.1.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ClintonElectronics\\DumpServer\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ClintonElectronics\\DumpServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
